
<form class="form-inline" action="POST" method="post" enctype="multipart/form-data">
      <div class="form-group">
              <label class="sr-only" for="firstName">Nombre</label>
              <input type="text" class="form-control " id="firstName" placeholder="nombre">
      </div>
      <br>
      <div class="form-group">
              <label class="sr-only" for="lastName">Apellidos</label>
              <input type="text" class="form-control" id="lastName" placeholder="Apellidos">
      </div>
      <br>
      <div class="form-group">
              <label class="sr-only" for="email">Email</label>
              <input type="email" class="form-control" id="email" placeholder="email">
      </div>
      <br>
      <div class="form-group">
            <label class="sr-only" for="login">login</label>
            <input type="text" class="form-control" id="login" placeholder="login">
      </div>
      <div class="form-group">
            <label class="sr-only" for="password">Contraseña</label>
            <input type="password" class="form-control" id="password" placeholder="contraseña">
      </div>
      <br>
      <br><br>
      <button type="submit" class="btn btn-info">Enviar</button>
</form>
