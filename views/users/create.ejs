
<form class="form-inline" action="/create" method="post" enctype="multipart/form-data">
  <div class="panel panel-info nuevoformulario">
      <div class="form-group">
              <label for="firstName">Nombre</label>
              <input type="text" class="form-control " id="firstName" name="firstName" placeholder="nombre">
      </div>
      <div class="form-group">
              <label for="lastName">Apellidos</label>
              <input type="text" class="form-control" id="lastName" name="lastName" placeholder="Apellidos">
      </div>
      <div class="form-group">
              <label for="dni">DNI</label>
              <input type="text" class="form-control" id="dni" name="dni" placeholder="dni">
      </div>
      <div class="form-group">
              <label  for="email">Email</label>
              <input type="email" class="form-control" id="email" name="mail" placeholder="email">
      </div>
      <div class="form-group">
              <label  for="phone">Teléfono</label>
              <input type="text" class="form-control" id="phone" name="phone" placeholder="phone">
      </div>
      <div class="form-group">
            <label  for="login">login</label>
            <input type="text" class="form-control" id="login" name="userName" placeholder="login">
      </div>
      <div class="form-group">
            <label for="password">Contraseña</label>
            <input type="password" class="form-control" id="password" name="password" placeholder="contraseña">
      </div>
      <div class="form-group">
        <label  for="userType">Permisos</label>
        <select class="form-control" id="userType" name="permissions" placeholder="permisos">
          <option>administrador</option>
          <option>colaborador</option>
          <option>revisor</option>
        </select>
      </div>
      <br>
      <button type="submit" class="btn btn-info">Enviar</button><br><br>
    </div>
</form>
<br>
<br>
<div class="backButton">
  <a href="/users" class="btn btn-info btn-lg active btn-block">Volver</a>
</div>
