<section data-ng-controller= "usersController">
<div class="sidebar-pane" id="profile">
      <br>
      <div class="panel panel-info">
          <div class="panel-heading">
              <h3 class="panel-title">New USER</h3>
          </div>
          <div class="panel-body">
              <p class="lorem">Introduzca la información del nuevo usuario</p>
                   <form class="form-inline" action="" method="post" enctype="multipart/form-data">
                      <div class="form-group">
                        <label class="sr-only" for="firstName">Nombre</label>
                        <input type="text" class="form-control " id="firstName" placeholder="nombre">
                      </div>
                      <br>
                      <div class="form-group">
                        <label class="sr-only" for="lastName">Apellidos</label>
                        <input type="text" class="form-control" id="lastName" placeholder="Apellidos">
                      </div>
                      <br>
                      <div class="form-group">
                        <label class="sr-only" for="email">Email</label>
                        <input type="text" class="form-control" id="email" placeholder="email">
                      </div>
                      <br>
                      <div class="form-group">
                        <label class="sr-only" for="login">login</label>
                        <input type="text" class="form-control" id="login" placeholder="login">
                      </div>
                      <div class="form-group">
                        <label class="sr-only" for="password">Contraseña</label>
                        <input type="text" class="form-control" id="password" placeholder="contraseña">
                      </div>
                      <br>
                      <br><br>
                      <button type="submit" class="btn btn-info">Enviar</button>
                 </form>
            </div>
      </div>
</div>
