<!DOCTYPE html>
<html>
<head>
    <title></title>

    <meta name="viewport" content="width=user-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no">
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/css/bootstrap.min.css">
    <link href="http://maxcdn.bootstrapcdn.com/font-awesome/4.4.0/css/font-awesome.min.css" rel="stylesheet">
    <link rel="stylesheet" href="http://cdn.leafletjs.com/leaflet-0.7.2/leaflet.css" />
    <link rel="stylesheet" href="../stylesheets/leaflet-sidebar.css" />
    <link rel="stylesheet" href="../stylesheets/geodesa.css" />
    <style>
        body {
            padding: 0;
            margin: 0;
        }
        html, body, #map {
            height: 100%;
            font: 10pt "Helvetica Neue", Arial, Helvetica, sans-serif;
        }
        .lorem {
            font-style: italic;
            color: #AAA;
        }
    </style>
</head>
<body>
  <div class="panel panel-info panel-listado">
      <div class="panel-heading">
            <h3 class="panel-title"><p>Revisiones</p></h3>
      </div>
         <table class="table">
           <thead>
             <tr>
               <th>
                 Fecha
               </th>
               <th>
                 Estado
               </th>
               <th>
                 Revisor
               </th>
             </tr>

           </thead>
           <tbody>
           <% for (var i=0; i< device.revisions.length; i++) { %>
               <tr>
                 <td>
                   <%= device.revisions[i].date %>
                 </td>
                 <td>
                  <%= device.revisions[i].id_estado %>
                 </td>
                 <td>
                  <%= device.revisions[i].id_caso %>
                 </td>

                 <td>
                <!--<form action="/devices/<%= devices.revisions[i]._id %>/edit" method="get">
                          <button type="submit" class="btn btn-warning">
                            <span class="glyphicon glyphicon-pencil"></span>
                          </button>
                      </form>-->
                 </td>
                 <td>
                    <!--  <form method= "post" action="/devices/<%= devices.revisions[i]._id %>?_method=delete">
                          <button type="submit" class="btn btn-danger">
                            <span class="glyphicon glyphicon-trash"></span>
                          </button>
                      </form>-->
                 </td>
              </tr>
            <% } %>
            </tbody>
          </table>
      </div>
      <div class="buttonPrimary">
        <!--  <form action="/users/new" method="get">
            <button type="submit" class="btn btn-primary">Crear nuevo usuario</button>
          </form>-->
      </div>
      <hr>
      <div class="backButton">
        <a href="/" class="btn btn-info btn-lg active btn-block">Volver</a>
      </div>


       <script src="http://cdn.leafletjs.com/leaflet-0.7.2/leaflet.js"></script>
       <script src="../../js/leaflet-sidebar.js"></script>
    </div>
</body>
